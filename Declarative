pipeline {
  agent any
  stages {
    stage("Build stage") {
      steps {
        echo "Build the applicaton"
      }  
    }
     stage("Test stage") {
      steps {
        echo ("Test the applicaton"
      }  
    }
     stage("Deploy stage") {
      steps {
        echo "Deploy the applicaton"
      }  
    }
  }
}
